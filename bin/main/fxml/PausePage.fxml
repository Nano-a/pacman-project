<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.Button ?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane id="PauseBackground" stylesheets="/css/style.css"  prefHeight="600.0" prefWidth="870.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.PausePageController">
<children>
    <VBox layoutX="290.0" layoutY="250.0" prefHeight="255.0" prefWidth="300.0">
        <children>
            <Button id="menuButton" fx:id="Resume" onAction="#resumeGame" prefWidth="400.0" text="RESUME">
                <VBox.margin>
                    <Insets top="60.0" right="30.0" left="30.0" />
                </VBox.margin>
            </Button>
            <Button id="menuButton" fx:id="Restart" onAction="#restartGame" prefWidth="400.0" text="RESTART">
                <VBox.margin>
                    <Insets top="20.0" right="30.0" left="30.0" />
                </VBox.margin>
            </Button>
            <Button id="menuButton" fx:id="Menu" onAction="#backToMenu" prefWidth="400.0" text="MENU">
                <VBox.margin>
                    <Insets top="20.0" right="30.0" left="30.0" />
                </VBox.margin>
            </Button>
            <Button id="menuButton" fx:id="Quit" onAction="#quitGame" prefWidth="400.0" text="QUIT">
                <VBox.margin>
                    <Insets top="20.0" right="30.0" left="30.0" />
                </VBox.margin>
            </Button>
        </children>
    </VBox>
</children>
</AnchorPane>
